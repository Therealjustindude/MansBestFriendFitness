 <%=f.label :name%>
    <%=f.text_field :name, placeholder: (dog.errors[:name].first || "Enter Name")%><br>
    <%=f.label :breed%>
    <%=f.text_field :breed, placeholder: (dog.errors[:breed].first || "Enter Breed")%><br>
    <%=f.label :age%>
    <%=f.number_field :age, placeholder: (dog.errors[:age].first || "Enter Age")%><br>
    <%=f.label :weight%>
    <%=f.number_field :weight, placeholder: "Enter Weight"%><br>
    <%=f.label :gender%>
    <%=f.text_field :gender, placeholder: (dog.errors[:gender].first || "Enter Gender")%><br>
    <%=f.label :intact, 'Check if your pet is not spayed/nuetered'%>
    <%=f.check_box :intact%><br>
    <%=f.label :birthday%>
    <%=f.date_field :birthday%><br>