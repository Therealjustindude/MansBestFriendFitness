<%=form_for [current_user, @user_dog, @journal_entry] do |f|%>
    
        <%= render partial: "journal_entry", locals: {f: f, user_dog: @user_dog, journal_entry: @journal_entry}%>
    
    <br>
        <fieldset>
            <legend>Exercise Entries</legend>
             <%=f.fields_for :exercise_entries do |exercise_entry|%>

                <%= render partial: "exercise_entry", locals: {exercise_entry: exercise_entry}%>
                
            <%end%>
        </fieldset>
    <br>
        <fieldset>
            <legend>Diet Entry</legend>
                <%=f.fields_for :diet_entries do |diet_entry|%>

                    <%= render partial: "diet_entry", locals: {diet_entry: diet_entry}%>

                    
                    
                <%end%>
         </fieldset>
    <br>
    <%=f.submit%>
<%end%>