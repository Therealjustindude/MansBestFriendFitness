<%=form_for [current_user, @user_dog, @new_je] do |f|%>
    <%=f.label :title%>
    <%=f.text_field :title%>
    <%=f.label :entry%>
    <%=f.text_area :entry%>
    <%=f.label :date%>
    <%=f.date_field :date%>
   
    <%=f.fields_for :exercise_entries do |exercise_entry|%>
        <fieldset>
        <legend>Exercise Entry</legend>
        <%=exercise_entry.label :name%>
        <%=exercise_entry.text_field :name%>
        <%=exercise_entry.label :duration%>
        <%=exercise_entry.time_field :duration%>
        <%=exercise_entry.label :description%>
        <%=exercise_entry.text_area :description%>
    <%end%>
        </fieldset>
    
    <%=f.fields_for :diet_entries do |diet_entry|%>
        <fieldset>
        <legend>Diet Entry</legend>
        <%=diet_entry.label :food%>
        <%=diet_entry.text_area :food%>
        <%=diet_entry.label :snacks%>
        <%=diet_entry.text_area :snacks%>
        <%=diet_entry.label :water%>
        <%=diet_entry.text_area :water%>
        <%=diet_entry.label :calories%>
        <%=diet_entry.number_field :calories%>
    <%end%>
        </fieldset>
    <%=f.submit%>
<%end%>