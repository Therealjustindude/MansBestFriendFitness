<div class="journal-entry-show">
    <h4>Journal Entry</h4>
    <fieldset>
    <h3><%= @journal_entry.title%></h3>

    
        <%=@journal_entry.entry%>
        <br>
        <br>
        Journal entry for: <%=@journal_entry.dog.name.capitalize%>
        Authored by: <%=@journal_entry.user.full_name%>
    </fieldset>
   

    <fieldset>

    <h5>Diet Entry</h5>
    <%@journal_entry.diet_entries.each do |diet_entry|%>
        Food: <%=diet_entry.food%><br>
        Water: <%=diet_entry.water%><br>
        Snacks: <%=diet_entry.snacks%><br>
        Calories: <%=diet_entry.calories%><br>

    <%end%><br>
    
    <h5>Exercise Entry</h5>
    <%@journal_entry.exercise_entries.each do |exercise_entry|%>
        
        Name: <%=exercise_entry.name%><br>
        Description: <%=exercise_entry.description%><br>
        Duration: <%=exercise_entry.duration%><br>
    <%end%><br>
    </fieldset>
   



<%if can? :update, @journal_entry%><%=link_to "Edit Journal Entry", edit_user_dog_journal_entry_path(@journal_entry.user_id, @journal_entry.dog_id, @journal_entry)%><%end%>|

 <%if can? :destroy, @journal_entry%><%=link_to "Delete ", user_dog_journal_entry_path(@journal_entry.user_id, @journal_entry.dog_id, @journal_entry), method: 'delete'%><%end%><br>
    
</div>